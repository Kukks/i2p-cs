{
    "version": "0.2.0",
    "configurations": [
        {
            // Use IntelliSense to find out which attributes exist for C# debugging
            // Use hover for the description of the existing attributes
            // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
            "name": "I2PDemo (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "builddemo",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Samples/I2PDemo/bin/Debug/net5.0/I2PDemo.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Samples/I2PDemo",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "I2PRouter (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildrouter",
            "program": "${workspaceFolder}/Samples/I2PRouter/bin/Debug/net5.0/I2PRouter.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Samples/I2PRouter",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "I2PEchoServer (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildeserver",
            "program": "${workspaceFolder}/Samples/I2PEchoServer/bin/Debug/net5.0/I2PEchoServer.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Samples/I2PEchoServer",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "I2PEchoClient (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildeclient",
            "program": "${workspaceFolder}/Samples/I2PEchoClient/bin/Debug/net5.0/I2PEchoClient.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Samples/I2PEchoClient",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}